{
  "schema": "ais-plan-skeleton/0.0.1",
  "meta": {
    "name": "swap",
    "description": "quote then swap"
  },
  "default_chain": "eip155:1",
  "nodes": [
    {
      "id": "quote",
      "type": "query_ref",
      "protocol": "demo@0.0.2",
      "query": "quote",
      "args": {
        "amount_in": { "ref": "inputs.amount_in" }
      }
    },
    {
      "id": "swap",
      "type": "action_ref",
      "protocol": "demo@0.0.2",
      "action": "swap",
      "deps": ["quote"],
      "args": {
        "amount_in": { "ref": "inputs.amount_in" },
        "min_out": { "ref": "nodes.quote.outputs.amount_out" }
      }
    }
  ],
  "policy_hints": {
    "risk_preference": "low"
  }
}

