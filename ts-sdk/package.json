{
  "name": "@ais-protocol/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for parsing and validating AIS (Agent Interaction Specification) files",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ais",
    "defi",
    "agent",
    "protocol",
    "yaml",
    "sdk"
  ],
  "author": "AIS Protocol",
  "license": "MIT",
  "dependencies": {
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
